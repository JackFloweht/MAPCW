<script>
document.addEventListener("DOMContentLoaded", function() {
    const terrainTextures = {
        'ğŸŒ²': '1.png',
        'ğŸ”': '14.png',
        'ğŸŒ»': '5.png',
        'ğŸ›': '11.png',
        'ğŸ”¥': '8.png',
        'ğŸ›–': '4.png',
        'â›²ï¸': '6.png',
        'â›ºï¸': '10.png',
        'ğŸ•³': '9.png',
        'ğŸŸ': '12.png',
        'ğŸ—¼': '15.png',
        'ğŸ°': '16.png',
        'ğŸ—¿': '7.png',
        'ğŸŒ³': '17.png',
    };

    const basePath = './';
    const defaultTexture = '5.png';

    document.querySelectorAll('.map-cell').forEach(cell => {
        // Obtener el emoji del contenido textual
        const emoji = cell.textContent.trim(); 
        const texture = terrainTextures[emoji] || defaultTexture;
        cell.style.backgroundImage = `url('${basePath}${texture}')`;
        
        // Limpiar el contenido del emoji despuÃ©s de cargar la imagen
        cell.textContent = '';

        // Efecto hover
        cell.addEventListener('mouseenter', () => {
            cell.style.transform = 'scale(1.1)';
            cell.style.zIndex = '1';
        });
        
        cell.addEventListener('mouseleave', () => {
            cell.style.transform = 'scale(1)';
            cell.style.zIndex = '0';
        });
    });
});
</script>
